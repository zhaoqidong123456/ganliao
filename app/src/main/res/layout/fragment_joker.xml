            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/mJoker_recy">
        </androidx.recyclerview.widget.RecyclerView>
    </androidx.swiperefreshlayout.widget.SwipeRefreshLayout>
</LinearLayout>